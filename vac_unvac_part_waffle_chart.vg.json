{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 700,
    "data": {
        "name": "vaccine",
        "url": "clean_data/clean_total_vaccine.csv"
    },
    "scales": [
      {
        "name": "x",
        "type": "quantitative",
        "domain": {"data": "vaccine", "field": "cumul_full"},
        "range": "width"
      }, {
        "name": "y",
        "type": "quantitative",
        "domain": {"data": "vaccine", "field": "cumul_partial"},
        "range": "height",
        "round": false
      }
    ],
    "marks": [
      {
        "type": "rect",
        "from": {"data": "vaccine"},
        "properties": {
          "enter": {
            "x": {"scale": "x", "field": "date"},
            "width": {"value": 20},
            "y": {"scale": "y", "field": "hour"},
            "height": {"value": 20},
            "shape": { "value": "circle" },
            "fill": {"scale": "c", "field": "temp"}
          }
        }
      }
    ]
  }
